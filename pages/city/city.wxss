.container{
  padding: 0 40rpx;
}

.list{
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 120rpx;
  padding: 35rpx 24rpx;
  margin-top: 24rpx;
  box-shadow:0px 4px 12px 0px rgba(164,178,190,0.5);
  border-radius: 16rpx;
  box-sizing: border-box;
}

.list .text{
  font-size: 36rpx;
  line-height: 50rpx;
  letter-spacing: 1rpx;
  color: #3C3C3C;
}

.list .radio{
  width: 36rpx;
  height: 36rpx;
}

.list .radio image{
  width: 100%;
  height: 100%;
  vertical-align: top;
}