.container{
  padding: 0 40rpx;
}

.list{
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  padding: 40rpx 0;
  border-bottom: 1rpx solid #e0e0e0;
}

.list .text{
  font-size: 32rpx;
  line-height: 45rpx;
  color: #3C3C3C;
}

.list .arrow_icon{
  width: 28rpx;
  height: 28rpx;
}

.list .arrow_icon image{
  width: 100%;
  height: 100%;
  vertical-align: top;
}